<h2 class="govuk-heading-l">Search results</h2>
<p class="govuk-hint">Showing {{control.data.pageSize}} of {{control.data.total}} {% if control.data.total === 1 %}result{% else %}results{% endif %}</p>
<table class="govuk-table">
    <thead class="govuk-table__head">
    <tr class="govuk-table__row">
        <th scope="col" class="govuk-table__header">Order number</th>
        <th scope="col" class="govuk-table__header">Email</th>
        <th scope="col" class="govuk-table__header">Company number</th>
        <th scope="col" class="govuk-table__header">Order type</th>
        <th scope="col" class="govuk-table__header">Order date</th>
        <th scope="col" class="govuk-table__header">Payment status</th>
    </tr>
    </thead>
    <tbody class="govuk-table__body">
        {% for control in control.controls %}
            {% include control.template %}
        {% endfor %}
    </tbody>
</table>
