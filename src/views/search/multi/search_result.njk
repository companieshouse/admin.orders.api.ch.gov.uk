<tr class="govuk-table__row">
    <td class="govuk-table__cell">
        {% if control.data.detailHref %}
            <a class="govuk-link" href="{{control.data.detailHref}}">{{ control.data.id }}</a>
        {% else %}
            {{ control.data.id }}
        {% endif %}
    </td>
    <td class="govuk-table__cell">{{ control.data.email }}</td>
    <td class="govuk-table__cell">{{ control.data.orderDate }}</td>
    <td class="govuk-table__cell">{{ control.data.paymentStatus }}</td>
</tr>
